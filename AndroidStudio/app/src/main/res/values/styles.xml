<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="textInputETLayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textColorHint">@color/greyGuardian</item>
        <item name="android:textColor">@color/black</item>
        <item name="boxStrokeColor">@color/yellowbtn</item>
    </style>
    <style name="GuardianTextInputStyle" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="boxStrokeWidth">2dp</item>
        <item name="boxStrokeColor">@color/yellowbtn</item>
        <item name="android:textColorHint">@color/blue_nav</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="textInputSCadastro" parent="GuardianTextInputStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColorHint">@color/white</item>
    </style>
    <style name="textInputETCadastro">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/greyGuardian</item>
    </style>
    <style name="TimePriceBox">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/time_price_box</item>
        <item name="android:elevation">8dp</item>
        <item name="android:gravity">center</item>
        <item name="android:orientation">vertical</item>
        <item name="android:padding">50dp</item>
    </style>
</resources>